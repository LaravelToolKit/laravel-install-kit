{
  "name": "shawnsandy/laravel-install-kit",
  "type": "library",
  "description": "A simple starter package for creating composer packages, based on php-league skelton",
  "keywords": [
    "shawnsandy",
    "laravel-install-kit"
  ],
  "homepage": "https://github.com/LaravelToolKit/laravel-install-kit",
  "license": "MIT",
  "authors": [
    {
      "name": "shawn sandy",
      "email": "shawnsandy04@gmail.com",
      "homepage": "http://shawnsandy.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/LaravelToolKit/simple_pack.git"
    }
  ],
  "require": {
    "php": "~5.5|~7.0",
    "barryvdh/laravel-debugbar": "^2.2",
    "laravelcollective/html": "^5.2",
    "doctrine/dbal": "^2.5",
    "barryvdh/laravel-ide-helper": "^2.1",
    "maatwebsite/excel": "^2.1",
    "spatie/laravel-analytics": "^1.3",
    "spatie/laravel-medialibrary": "^3.17",
    "spatie/laravel-permission": "^1.3",
    "spatie/laravel-authorize": "^1.1",
    "spatie/geocoder": "^2.1",
    "mpociot/laravel-test-factory-helper": "^0.2.0",
    "laravel/socialite": "^2.0",
    "spatie/googlesearch": "^2.0",
    "styde/html": "~1.1",
    "laracasts/utilities": "^2.1",
    "jeroen-g/laravel-packager": "^1.4",
    "laracasts/flash": "^2.0",
    "laracasts/generators": "^1.1",
    "cviebrock/eloquent-sluggable": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"

  },
  "autoload": {
    "psr-4": {
      "ShawnSandy\\InstallKit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ShawnSandy\\InstallKit\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
