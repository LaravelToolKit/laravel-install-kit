{
  "name": "shawnsandy/laravel-install-kit",
  "type": "library",
  "description": "A simple starter package for creating composer packages, based on php-league skelton",
  "keywords": [
    "shawnsandy",
    "laravel-install-kit"
  ],
  "homepage": "https://github.com/shawnsandy/laravel-install-kit",
  "license": "MIT",
  "authors": [
    {
      "name": "shawn sandy",
      "email": "shawnsandy04@gmail.com",
      "homepage": "http://shawnsandy.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/LaravelToolKit/simple_pack.git"
    }
  ],
  "require": {
    "php": "~5.5|~7.0",
    "shawnsandy/simple_pack": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"

  },
  "autoload": {
    "psr-4": {
      "ShawnSandy\\InstallKit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ShawnSandy\\InstallKit\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
